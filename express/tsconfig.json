{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "express/src",
    "paths": {
      "@/*": ["./express/src/*"]
    }
  },
  "include": ["express/src/**/*"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
